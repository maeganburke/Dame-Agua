<div class="container" id="index">
		<div>
			<h1>Current Dame Agua Users</h1>
			<div id="divider-left"></div>
			<b><p> name | username | email </p></b>
			<div id="divider-left"></div>
			<% @users.each do  |user| %>
				<p id="user-list">
					<%= user.name %> |
					<%= user.username %> |
					<%= user.email %> |
					<%= button_to "delete user", admin_user_path(user), :method => "delete", :style => 'color: #b81f00', :class => 'btn-link', :id => 'create-user' %> |
					<%= link_to "edit user", edit_admin_user_path(user), :class => 'btn-link', :id => 'edit-user' %>
				</p>
			<% end %>
		</div>
		<div id="divider-left"></div>
		<%= link_to 'create new user', new_admin_user_path %>
</div>
